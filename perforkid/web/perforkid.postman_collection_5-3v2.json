			"item": [
				{
					"name": "‚úÖ signIn",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Check if the request body is present and has a 'raw' property\r",
									"if (pm.request.body && pm.request.body.raw) {\r",
									"    try {\r",
									"        const responseBody = JSON.parse(pm.request.body.raw);\r",
									"\r",
									"        // Check if 'username' and 'password' properties exist in the JSON body\r",
									"        if (responseBody && responseBody.password) {\r",
									"            const password = responseBody.password;\r",
									"\r",
									"            pm.globals.set(\"password\", password);\r",
									"        } else {\r",
									"            console.error(\"Missing 'username' or 'password' properties in the request body.\");\r",
									"        }\r",
									"    } catch (error) {\r",
									"        console.error(\"Error parsing JSON from the request body:\", error.message);\r",
									"    }\r",
									"} else {\r",
									"    console.error(\"Request body or 'raw' property is undefined.\");\r",
									"}\r",
									"\r",
									"const responseBody1 = pm.response.json();\r",
									"const value = responseBody1.token;\r",
									"\r",
									"pm.globals.set(\"token\", value);\r",
									"\r",
									"pm.test(\"Response status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Response is an object with token, type, id, username, email, and roles properties\", function () {\r",
									"    var responseBody = pm.response.json();\r",
									"    pm.expect(responseBody).to.be.an('object').that.includes.all.keys('token', 'type', 'id', 'username', 'email', 'roles');\r",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
